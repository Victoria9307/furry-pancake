package selfStudy;

import java.util.Scanner;

public class BasicOperators2 {

	public static void main(String[] args) {
		Scanner input;
		input = new Scanner(System.in);
		
		System.out.printf("First Number?: ");
		int a = input.nextInt();
		System.out.printf("Second Number?: ");
		int b = input.nextInt();
		System.out.printf("Third Number?: ");
		int c = input.nextInt();
		
		if (a < b) {
			System.out.print(1);}
		else if (a > b) {
			 if (a > c) {
				System.out.print(0);}
			 else {
				 System.out.print(1);}
			 }
		else {
			System.out.print(0);
			}	
		}
	}	
