package selfStudy;

import java.util.Scanner;

public class Condition3 {

		public static void main(String[] args) {
						
			Scanner scan = new Scanner(System.in);
			
			System.out.printf("Number?: ");
			int number = scan.nextInt();
			String animal;	
			
			switch (number) {
			case 1: 
				animal = "Dog";
				break;
			case 2:
				animal = "Cat";
				break;
			case 3:
				animal = "Chick";
				break;
			default:
				animal = "Try again";
				break;
				
			}
			
			System.out.println(animal);
	}

}
